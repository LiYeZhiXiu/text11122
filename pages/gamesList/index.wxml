<!--pages/gamesList/index.wxml-->
<view class="page">
<view class="cu-card article no-card margin-top-sm" wx:for="{{list}}" data-id="{{item.id}}">
  <view class="cu-item shadow">
    <view class="title">
      <view class="text-cut">{{gameName}}</view>
    </view>
    <view class="content">
      <view class="desc">
        <view class="text-content">{{item.title}}</view>
      </view>
    </view>
  </view>
</view>
</view>
